{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowELBPutLogs",
      "Effect": "Allow",
      "Principal": {
        "Service": "elasticloadbalancing.amazonaws.com"
      },
      "Action": [
        "s3:PutObject"
      ],
  "Resource": "arn:aws:s3:::balteen121-elb-logs/*"
    },
    {
      "Sid": "AllowELBGetBucketLocation",
      "Effect": "Allow",
      "Principal": {
        "Service": "elasticloadbalancing.amazonaws.com"
      },
      "Action": [
        "s3:GetBucketLocation"
      ],
  "Resource": "arn:aws:s3:::balteen121-elb-logs"
    }
  ]
}
{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowJarAccessForEC2Role",
      "Effect": "Allow",
      "Principal": {
        "AWS": "*"
      },
      "Action": [
        "s3:GetObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::${app_bucket_name}",
        "arn:aws:s3:::${app_bucket_name}/*"
      ]
    }
  ]
}